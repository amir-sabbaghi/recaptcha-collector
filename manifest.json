{
	"manifest_version": 2,
	"name": "reCaptcha Collector",
	"version": "1.0",
	"description": "Collects data for training a neural network to solve reCaptcha",
	"icons": {
		"48": "icons/border-48.png"
	},
	"premissions": [
		"http://localhost:12345",
		"webRequest"
	],
	"content_scripts": [
		{
			"matches": ["*://www.google.com/recaptcha/api2/bframe*"],
			"all_frames": true,
			"js": ["detector.js"]
		}
	]
}
